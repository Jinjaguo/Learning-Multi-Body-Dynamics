<?xml version="1.0" ?>
<robot name="disk">
  <link name="baseLink">
    <contact>
      <friction_anchor/>
      <lateral_friction value="0.1"/>
    </contact>

    <inertial>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <mass value="10"/>
      <inertia ixx="1" ixy="0" ixz="0" iyy="1" iyz="0" izz="1"/>
    </inertial>

    <visual>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
        <cylinder radius="0.07" length="0.05"/>
      </geometry>
      <material name="blue">
        <color rgba="1 1 1 1"/>
      </material>
    </visual>

    <collision>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
        <cylinder radius="0.07" length="0.05"/>
      </geometry>
    </collision>
  </link>
</robot>
